[MASTER]
jobs=4

[MESSAGES CONTROL]
disable=fixme,ungrouped-imports,wrong-import-order,method-hidden

[REPORTS]
output-format=parseable
reports=yes
score=yes

[BASIC]
include-naming-hint=yes

[FORMAT]
expected-line-ending-format=LF
max-line-length=79

[MISCELLANEOUS]
notes=FIXME,XXX,TODO,HACK

[CLASSES]
exclude-protected=_options,_add,_finish,_name,_request

[DESIGN]
min-public-methods=0

[EXCEPTIONS]
overgeneral-exceptions=
